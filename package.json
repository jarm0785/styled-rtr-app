{
  "name": "styled-rtr-app",
  "version": "0.0.1",
  "description": "Scalable template to create modern web applications using React + Typescript + Redux using Styled Components.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development --open --config ./webpack.dev.js",
    "build": "webpack --mode production --config ./webpack.config.js",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint src/**/*.{js,ts,tsx} --quiet",
    "lint:style": "stylelint src/**/*.scss",
    "lint:fix": "eslint src/**/*.{js,ts,tsx} --fix ",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "typescript",
    "redux",
    "redux-toolkit",
    "react-router",
    "react-router-dom",
    "axios",
    "jest",
    "react testing library",
    "storybook",
    "boilerplate",
    "webpack 5",
    "template",
    "react fast refresh"
  ],
  "author": "jarm0785",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",

    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/chai": "^4.2.18",
    "@types/jest": "^25.2.3",
    "@types/loadable__component": "^5.13.3",
    "@types/node": "^15.12.1",
    "@types/react": "^16.14.2",
